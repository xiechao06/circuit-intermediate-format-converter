{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "SchematicData",
    "type": "object",
    "properties": {
        "library": {
            "type": "array",
            "description": "模型库，包含所有元件的定义, 对应KiCad Schematic的lib_symbols表达式",
            "items": {
                "type": "object",
                "additionalProperties": {
                    "type": "object",
                    "properties": {
                        "type": {
                            "type": "string"
                        },
                        "ref": {
                            "type": "string",
                            "description": "元件的参考标识符，对应KiCad Schematic的Reference属性"
                        },
                        "package": {
                            "type": "string",
                            "description": "元件封装，对应KiCad Schematic的Footprint属性"
                        },
                        "pins": {
                            "type": "array",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "id": {
                                        "type": "string"
                                    },
                                    "name": {
                                        "type": "string",
                                        "description": "引脚名称，对应pin表达式的Name字段"
                                    },
                                    "type": {
                                        "type": "string",
                                        "description": "引脚类型，如power_in、power_out、bidirectional、passive等"
                                    },
                                    "rel_x": {
                                        "type": "number"
                                    },
                                    "rel_y": {
                                        "type": "number"
                                    },
                                    "angle": {
                                        "type": "number",
                                        "description": "引脚与水平向右方向的夹角，单位为度，逆时针为正"
                                    }
                                },
                                "required": [
                                    "id",
                                    "name",
                                    "type",
                                    "rel_x",
                                    "rel_y"
                                ],
                                "additionalProperties": false
                            }
                        }
                    },
                    "required": [
                        "type",
                        "ref",
                        "pins"
                    ],
                    "additionalProperties": false
                }
            }
        },
        "instances": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "uuid": {
                        "type": "string"
                    },
                    "lib_id": {
                        "type": "string"
                    },
                    "designator": {
                        "type": "string"
                    },
                    "placement": {
                        "type": "object",
                        "properties": {
                            "x": {
                                "type": "number"
                            },
                            "y": {
                                "type": "number"
                            },
                            "rotation": {
                                "type": "number"
                            },
                            "mirror": {
                                "type": "boolean"
                            }
                        },
                        "required": [
                            "x",
                            "y",
                            "rotation",
                            "mirror"
                        ],
                        "additionalProperties": false
                    },
                    "attributes": {
                        "type": "object",
                        "properties": {
                            "Value": {
                                "type": "string"
                            },
                            "Tolerance": {
                                "type": "string"
                            },
                            "Firmware": {
                                "type": "string"
                            },
                            "description": {
                                "type": "string"
                            }
                        },
                        "additionalProperties": true
                    }
                },
                "required": [
                    "uuid",
                    "lib_id",
                    "designator",
                    "placement",
                    "attributes"
                ],
                "additionalProperties": false
            }
        },
        "wires": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "wire_id": {
                        "type": "string"
                    },
                    "segments": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "x": {
                                    "type": "number"
                                },
                                "y": {
                                    "type": "number"
                                }
                            },
                            "required": [
                                "x",
                                "y"
                            ],
                            "additionalProperties": false
                        }
                    },
                    "connects": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "ref": {
                                    "type": "string"
                                },
                                "pin": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "ref",
                                "pin"
                            ],
                            "additionalProperties": false
                        }
                    }
                },
                "required": [
                    "wire_id",
                    "segments",
                    "connects"
                ],
                "additionalProperties": false
            }
        },
        "nets": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "net_id": {
                        "type": "string"
                    },
                    "name": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ]
                    },
                    "nodes": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "ref": {
                                    "type": "string"
                                },
                                "pin": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "ref",
                                "pin"
                            ],
                            "additionalProperties": false
                        }
                    }
                },
                "required": [
                    "net_id",
                    "name",
                    "nodes"
                ],
        "additionalProper